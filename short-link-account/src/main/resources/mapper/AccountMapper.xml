<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hutu.shortlinkaccount.mapper.AccountMapper">

    <resultMap id="BaseResultMap" type="com.hutu.shortlinkaccount.domain.Account">
            <id property="id" column="id" />
            <result property="accountNo" column="account_no" />
            <result property="headImg" column="head_img" />
            <result property="phone" column="phone" />
            <result property="pwd" column="pwd" />
            <result property="secret" column="secret" />
            <result property="mail" column="mail" />
            <result property="username" column="username" />
            <result property="auth" column="auth" />
            <result property="gmtCreate" column="gmt_create" />
            <result property="gmtModified" column="gmt_modified" />
    </resultMap>

    <sql id="Base_Column_List">
        id,account_no,head_img,phone,pwd,secret,
        mail,username,auth,gmt_create,gmt_modified
    </sql>
</mapper>
